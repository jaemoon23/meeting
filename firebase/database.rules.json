{
  "rules": {
    "meetings": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "presence": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "templates": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "userTemplates": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid"
      }
    }
  }
}
